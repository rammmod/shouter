{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ShouterConfiguration": {
    "RabbitMQ": {
      "Hostname": "localhost",
      "Port": 5672,
      "VirtualHost": "/",
      "UserName": "shouter",
      "Password": "shouter",
      "Exchange": "Rhinero.Shouter",
      "PublishQueue": "Rhinero.Shouter.Publish",
      "ReplyQueue": "Rhinero.Shouter.Reply"
    },
    "Kafka": {
      "BootstrapServers": "localhost:9092",
      "PublishTopic": "Rhinero.Shouter.Publish",
      "RequestTopic": "Rhinero.Shouter.Request",
      "ReplyTopic": "Rhinero.Shouter.Reply",
      "ReplyGroup": "Rhinero.Shouter.Reply.Group",
      "PrefetchCount": 16,
      "ConcurrentMessageLimit": 16,
      "ConcurrentDeliveryLimit": 16,
      "ConcurrentConsumerLimit": 16,
      "Redis": {
        "ConnectionString": "localhost:6379",
        "CacheId": 0
      }
    },
    "Retry": {
      "Strategy": "Interval",
      "RetryCount": 5,
      "IntervalSeconds": 3
    }
  }
}
