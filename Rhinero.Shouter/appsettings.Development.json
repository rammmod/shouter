{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "RabbitMQ": {
    "Hostname": "localhost",
    "Port": 5672,
    "VirtualHost": "/",
    "UserName": "shouter",
    "Password": "shouter",
    "Queue": "Rhinero.Shouter",
    "PrefetchCount": 16,
    "ConcurrentMessageLimit": 16
  },
  "Serilog": {
    "WriteTo": [
      {
        "Name": "Elasticsearch",
        "Args": {
          "nodeUris": "http://shouter:shouter@localhost:9200",
          "indexFormat": "shouter-dev-{0:dd-MM-yyyy}",
          "restrictedToMinimumLevel": "Verbose",
          "bufferFileSizeLimitBytes": 5242880,
          "bufferLogShippingInterval": 5000,
          "bufferRetainedInvalidPayloadsLimitBytes": 5000,
          "autoRegisterTemplate": true,
          "autoRegisterTemplateVersion": "ESv7"
        }
      },
      {
        "Name": "Console"
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName" ],
    "Properties": {
      "Application": "Rhinero.Shouter"
    }
  }
}